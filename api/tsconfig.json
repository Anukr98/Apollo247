{
  "compilerOptions": {
    "outDir": "./dist/",
    "baseUrl": "./",
    "module": "es6",
    "target": "es6",
    "moduleResolution": "node",
    "allowJs": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "traceResolution": false,
    "allowUnreachableCode": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "lib": ["es5", "es6"]
  },
  "exclude": ["node_modules"]
}
