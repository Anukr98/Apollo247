{
  "name": "apollo-api",
  "version": "0.0.1",
  "description": "Apollo API",
  "main": "index.js",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "start:dev": "rm -rf dist && webpack --mode development && node dist/api.bundle.js",
    "format:fix": "prettier --write **/*.js **/*.jsx **/*.ts **/*.tsx **/*.json",
    "format:check": "prettier --list-different **/*.js **/*.jsx **/*.ts **/*.tsx **/*.json",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.134",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^5.16.0",
    "prettier": "^1.18.2",
    "ts-loader": "^6.0.2",
    "typescript": "~3.4.5",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "apollo-server": "^2.6.3",
    "lodash": "^4.17.11",
    "graphql": "^14.3.1"
  }
}
