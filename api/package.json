{
  "name": "apollo-api",
  "version": "0.0.1",
  "description": "Apollo API",
  "main": "index.js",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "init:doctors": "cd services/doctors && sequelize db:create && sequelize db:migrate",
    "start:dev": "webpack && (node dist/doctorsService.bundle.js &) && sleep 5 && node dist/api.bundle.js",
    "format:fix": "prettier --write **/*.js **/*.jsx **/*.ts **/*.tsx **/*.json",
    "format:check": "prettier --list-different **/*.js **/*.jsx **/*.ts **/*.tsx **/*.json",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx"
  },
  "devDependencies": {
    "@types/graphql": "^14.2.1",
    "@types/lodash": "^4.14.134",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.3",
    "prettier": "^1.18.2",
    "sequelize-cli": "^5.5.0",
    "ts-loader": "^6.0.2",
    "typescript": "~3.4.5",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@apollo/federation": "^0.6.4",
    "@apollo/gateway": "^0.6.6",
    "apollo-server": "^2.6.3",
    "graphql": "~14.2.1",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.11",
    "mysql2": "^1.6.5",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.8.10"
  }
}
