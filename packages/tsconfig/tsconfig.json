{
  "compilerOptions": {
    "module": "es6",
    "target": "es5",
    "moduleResolution": "node",
    "allowJs": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "noEmitOnError": true,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "strict": true,
    "lib": ["es5", "es6"]
  }
}
