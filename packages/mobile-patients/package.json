{
  "name": "@aph/mobile-patients",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "npm run build:android & npm run build:ios",
    "build:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "build:ios": "rm -rf ios/bundle/; mkdir ios/bundle; react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/bundle/main.jsbundle --assets-dest ios/bundle",
    "start": "react-native start",
    "test": "jest",
    "format": "prettier --list-different '**/*.{js,jsx,ts,tsx,json,htm,html}'",
    "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx,json,htm,html}'",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "apollo:codegen": "apollo codegen:generate ./src/graphql/types/ --outputFlat --addTypename --localSchemaFile=./node_modules/@aph/api-schema/schema.json --target=typescript --queries=./src/graphql/**.ts --watch"
  },
  "devDependencies": {
    "@aph/api-schema": "file:../api-schema",
    "@aph/eslint-config": "file:../eslint-config",
    "@aph/prettier-config": "file:../prettier-config",
    "@aph/tsconfig": "file:../tsconfig",
    "@types/graphql": "^14.2.1",
    "@types/lodash": "^4.14.134",
    "@types/react": "^16.8.19",
    "@types/react-intl": "^2.3.18",
    "@types/react-native": "^0.57.63",
    "@types/react-navigation": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-jest": "24.8.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-plugin-react-native": "^3.7.0",
    "jest": "24.8.0",
    "metro-react-native-babel-preset": "0.54.1",
    "prettier": "^1.18.2",
    "react-native-typescript-transformer": "^1.2.12",
    "react-test-renderer": "16.8.3",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@aph/universal": "file:../universal",
    "@aph/mobile-ui-components": "file:../mobile-ui-components",
    "@aph/shared-ui-components": "file:../shared-ui-components",
    "apollo": "^2.13.1",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.2",
    "apollo-link-error": "^1.1.11",
    "apollo-link-http": "^1.5.15",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "react": "16.8.6",
    "react-apollo": "^2.5.6",
    "react-apollo-hooks": "^0.4.5",
    "react-intl": "^2.9.0",
    "react-native": "0.59.9",
    "react-native-android-sms-listener": "^0.7.0",
    "react-native-app-intro-slider": "^2.0.1",
    "react-native-confirmation-code-input": "^1.0.4",
    "react-native-elements": "^1.1.0",
    "react-native-firebase": "^5.5.4",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-modal-datetime-picker": "^7.4.2",
    "react-native-otp-textinput": "0.0.6",
    "react-native-popup-menu": "^0.15.6",
    "react-native-splash-screen": "^3.2.0",
    "react-native-vector-icons": "^6.5.0",
    "react-navigation": "^3.11.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
