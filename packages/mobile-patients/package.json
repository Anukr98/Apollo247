{
  "name": "@aph/mobile-patients",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "apollo:codegen": "apollo codegen:generate ./src/graphql/types/ --outputFlat --addTypename --endpoint=https://aph-staging-api.apollo247.com/graphql --target=typescript --queries=./src/graphql/**.ts",
    "build": "npm run build:android & npm run build:ios",
    "build:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "build:ios": "rm -rf ios/bundle/; mkdir ios/bundle; react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/bundle/main.jsbundle --assets-dest ios/bundle",
    "format": "prettier --list-different '**/*.{js,jsx,ts,tsx,json,htm,html}'",
    "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx,json,htm,html}'",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "start": "react-native start",
    "test": "jest",
    "typecheck": "tsc --project . --noEmit",
    "jetify": "jetify",
    "android-dev": "adb reverse tcp:8081 tcp:8081 && react-native run-android",
    "postinstall": "cat OTSubscriberViewManager.java > node_modules/opentok-react-native/android/src/main/java/com/opentokreactnative/OTSubscriberViewManager.java && cat opentok-react-native.podspec > node_modules/opentok-react-native/opentok-react-native.podspec && npx jetify"
  },
