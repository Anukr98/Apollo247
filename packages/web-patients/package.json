{
  "name": "@aph/web-patients",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.bundle.js",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "start": "npm run apollo:codegen:watch & webpack-dev-server",
    "build": "webpack",
    "format": "prettier --list-different **/*.js **/*.jsx **/*.ts **/*.tsx **/*.json",
    "format:fix": "prettier --write **/**/*.js **/**/*.jsx **/**/*.ts **/**/*.tsx **/**/*.json",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "apollo:download": "apollo schema:download ./src/graphql/schema.json --endpoint=http://api-gateway:${API_GATEWAY_PORT}/graphql",
    "apollo:codegen": "apollo codegen:generate ./src/graphql/types/ --outputFlat --addTypename --localSchemaFile=./src/graphql/schema.json --target=typescript --queries=./src/graphql/**.ts",
    "apollo:codegen:watch": "apollo codegen:generate ./src/graphql/types/ --outputFlat --addTypename --localSchemaFile=./src/graphql/schema.json --target=typescript --queries=./src/graphql/**.ts --watch"
  },
  "devDependencies": {
    "@aph/eslint-config": "file:../eslint-config",
    "@aph/prettier-config": "file:../prettier-config",
    "@aph/tsconfig": "file:../tsconfig",
    "@babel/core": "7.4.5",
    "@cypress/webpack-preprocessor": "^4.1.0",
    "@material-ui/pickers": "^3.1.2",
    "@types/graphql": "^14.2.1",
    "@types/lodash": "^4.14.134",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-router": "^5.0.1",
    "@types/react-router-dom": "^4.3.3",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cypress": "^3.3.1",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "lerna": "^3.15.0",
    "prettier": "^1.18.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.2",
    "url-loader": "^2.0.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1"
  },
  "dependencies": {
    "@aph/universal": "file:../universal",
    "@aph/web-ui-components": "file:../web-ui-components",
    "@date-io/date-fns": "^1.3.8",
    "@hot-loader/react-dom": "16.8.6",
    "@material-ui/core": "^4.0.2",
    "@material-ui/icons": "^4.0.1",
    "@material-ui/styles": "^4.0.2",
    "apollo": "^2.13.1",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.2",
    "apollo-link-context": "^1.0.18",
    "apollo-link-error": "^1.1.11",
    "apollo-link-http": "^1.5.15",
    "date-fns": "^2.0.0-beta.2",
    "firebase": "^6.2.2",
    "firebaseui": "^4.0.0",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.11",
    "react": "16.8.6",
    "react-apollo": "^2.5.6",
    "react-apollo-hooks": "^0.4.5",
    "react-dom": "16.8.6",
    "react-hot-loader": "^4.9.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "unfetch": "^4.1.0"
  }
}
