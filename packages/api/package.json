  "scripts": {
    "start:profiles-service": "(mkdir -p dist && test -f dist/profiles-service.bundle.js || echo 'module.exports={}' > dist/profiles-service.bundle.js) && webpack profiles-service=profiles-service/profiles-service.ts --watch & nodemon dist/profiles-service.bundle.js --watch dist/profiles-service.bundle.js",
    "start:doctors-service": "(mkdir -p dist && test -f dist/doctors-service.bundle.js || echo 'module.exports={}' > dist/doctors-service.bundle.js) && webpack doctors-service=doctors-service/doctors-service.ts --watch & nodemon dist/doctors-service.bundle.js --watch dist/doctors-service.bundle.js",
    "start:api-gateway": "webpack api-gateway=api-gateway.ts && node dist/api-gateway.bundle.js",
    "format:fix": "prettier --write **/*.js **/*.jsx **/*.ts **/*.tsx **/*.json",
    "format:check": "prettier --list-different **/*.js **/*.jsx **/*.ts **/*.tsx **/*.json",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx"
  },
  "devDependencies": {
    "@aph/eslint-config": "0.0.1",
