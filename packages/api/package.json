    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.3",
    "lerna": "^3.15.0",
    "prettier": "^1.18.2",
    "sequelize-cli": "^5.5.0",
