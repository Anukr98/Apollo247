{
  "name": "@aph/mobile-doctors",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "apollo:codegen": "apollo codegen:generate ./src/graphql/types/ --outputFlat --addTypename --localSchemaFile=./node_modules/@aph/api-schema/schema.json --target=typescript --queries=./src/graphql/**.ts --watch",
    "build": "npm run build:android & npm run build:ios",
    "build:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "build:ios": "rm -rf ios/bundle/; mkdir ios/bundle; react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/bundle/main.jsbundle --assets-dest ios/bundle",
    "format": "prettier --list-different '**/*.{js,jsx,ts,tsx,json,htm,html}'",
    "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx,json,htm,html}'",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "start": "react-native start",
    "test": "jest",
    "typecheck": "tsc --project . --noEmit",
    "jetify": "jetify"
  },
  "devDependencies": {
    "@aph/api-schema": "file:../api-schema",
    "@aph/eslint-config": "file:../eslint-config",
    "@aph/prettier-config": "file:../prettier-config",
    "@aph/tsconfig": "file:../tsconfig",
    "@types/graphql": "^14.2.1",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.134",
    "@types/react": "^16.8.1",
    "@types/react-intl": "^2.3.18",
    "@types/react-native": "^0.57.63",
    "@types/react-native-calendars": "^1.20.7",
    "@types/react-native-material-dropdown": "^0.11.0",
    "@types/react-native-swipe-gestures": "^1.0.0",
    "@types/react-navigation": "^3.0.7",
    "@types/react-test-renderer": "^16.8.1",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "@babel/core": "^7.5.0",
    "@babel/runtime": "^7.5.0",
    "babel-jest": "24.1.0",
    "@react-native-community/eslint-config": "^0.0.3",
    "eslint": "^5.16.0",
