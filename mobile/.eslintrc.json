{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [],
  "plugins": ["import", "@typescript-eslint"],
  "rules": {
    "import/no-default-export": "warn",
    "@typescript-eslint/no-unused-vars": ["warn", { "args": "none" }],
    "prefer-const": "warn"
  }
}
