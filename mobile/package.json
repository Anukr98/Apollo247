{
  "name": "apollo-mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "build:ios": "rm -rf ios/bundle/; mkdir ios/bundle; react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/bundle/main.jsbundle --assets-dest ios/bundle",
    "start:dev": "node node_modules/react-native/local-cli/cli.js start",
    "start:ios": "node_modules/.bin/react-native run-ios",
    "test": "jest",
    "format:fix": "prettier --write **/*.js **/*.jsx **/*.ts **/*.tsx **/*.json",
    "format:check": "prettier --list-different **/*.js **/*.jsx **/*.ts **/*.tsx **/*.json",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx"
  },
  "devDependencies": {
    "@types/auth0-js": "^9.10.2",
    "@types/lodash": "^4.14.134",
    "@types/react": "^16.8.19",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "babel-jest": "24.8.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-plugin-react-native": "^3.7.0",
    "jest": "24.8.0",
    "metro-react-native-babel-preset": "0.54.1",
    "prettier": "^1.18.2",
    "react-native-typescript-transformer": "^1.2.12",
    "react-test-renderer": "16.8.3",
    "typescript": "~3.4.5"
  },
  "dependencies": {
    "auth0-js": "^9.10.4",
    "graphql": "~14.2.1",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.11",
    "react": "16.8.3",
    "react-apollo": "^2.5.6",
    "react-apollo-hooks": "^0.4.5",
    "react-native": "0.59.9",
    "react-native-android-sms-listener": "^0.7.0",
    "react-native-app-intro-slider": "^2.0.1",
    "react-native-elements": "^1.1.0",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-vector-icons": "^6.5.0",
    "react-navigation": "^3.11.0",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
