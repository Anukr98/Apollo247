    "@hot-loader/react-dom": "16.8.6",
    "@material-ui/core": "^4.0.2",
    "@material-ui/icons": "^4.0.1",
    "@material-ui/styles": "^4.0.2",
    "lodash": "^4.17.11",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-hot-loader": "^4.9.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1"
