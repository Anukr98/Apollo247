{
  "compilerOptions": {
    "outDir": "./dist/",
    "baseUrl": "./",
    "module": "es6",
    "target": "es5",
    "moduleResolution": "node",
    "jsx": "react",
    "allowJs": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "traceResolution": false,
    "allowUnreachableCode": true,
    "strictNullChecks": true,
    "lib": ["dom", "dom.iterable", "es5", "es6", "scripthost"]
  },
  "exclude": ["node_modules"]
}
